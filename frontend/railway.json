{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "frontend/Dockerfile",
    "buildContext": "frontend"
  },
  "deploy": {
    "startCommand": "serve -s build -l 3000",
    "env": {
      "REACT_APP_API_URL": "https://<your-backend>.railway.app"
    }
  }
}
